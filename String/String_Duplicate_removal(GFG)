import java.util.*;

class Solution {
    String removeDuplicates(String s) {
        Set<Character> set = new LinkedHashSet<>(); // maintains order
        for (int i = 0; i < s.length(); i++) {
            set.add(s.charAt(i));
        }

        StringBuilder result = new StringBuilder();
        for (char ch : set) {
            result.append(ch);
        }

        return result.toString();
    }
}
