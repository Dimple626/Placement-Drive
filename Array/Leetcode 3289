class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int[] res=new int[2];
        Map<Integer,Integer>  map=new HashMap<>();
        for(int num:nums){
            map.put(num,map.getOrDefault(num,0)+1);
        }
        int i=0;
        
        for(Map.Entry<Integer,Integer> entry:map.entrySet()){
            if(entry.getValue()>1){
                res[i++]=entry.getKey();
            }
        }
        return res;
    }
}
